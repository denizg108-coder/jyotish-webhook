{
  "functions": {
    "api/webhook.py": {
      "runtime": "python3.9"
    },
    "api/health.py": {
      "runtime": "python3.9"  
    },
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/api/health", 
      "dest": "/api/health.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
